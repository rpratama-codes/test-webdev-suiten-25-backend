{
	"name": "test-webdev-suiten-25",
	"version": "1.0.0",
	"description": "Paywiz HRIS Backend",
	"homepage": "https://github.com/rpratama-codes/test-webdev-suiten-25#readme",
	"bugs": {
		"url": "https://github.com/rpratama-codes/test-webdev-suiten-25/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rpratama-codes/test-webdev-suiten-25.git"
	},
	"license": "BSD-3-Clause",
	"author": "Rizqi Pratama",
	"type": "module",
	"main": "src/index.js",
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"scripts": {
		"dev": "nodemon",
		"dev:email": "email dev --dir src/templates",
		"build": "rimraf ./dist && vite build --config vite.config.js",
		"build:tsc": "rimraf ./dist && tsc",
		"build:docker": "docker build -t rekomendasiin-app:latest .",
		"start": "node ./dist/index.js",
		"start:docker": "docker compose up app --build",
		"start:docker:daemon": "docker compose up app --build -d",
		"test:unit": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "biome check --write ./src",
		"prepare": "husky",
		"commit": "cz",
		"prisma:generate": "prisma generate",
		"prisma:migrate:dev": "prisma migrate dev",
		"prisma:migrate:deploy": "prisma migrate deploy",
		"prisma:db:seed": "prisma db seed",
		"encrypt": "dotenvx encrypt",
		"decrypt": "dotenvx decrypt"
	},
	"dependencies": {
		"@dotenvx/dotenvx": "^1.51.1",
		"@prisma/adapter-pg": "^7.0.0",
		"@prisma/client": "^7.0.0",
		"@react-email/components": "^0.5.7",
		"argon2": "^0.44.0",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"express-rate-limit": "^8.2.1",
		"google-auth-library": "^10.5.0",
		"helmet": "^8.1.0",
		"jose": "^6.1.0",
		"morgan": "^1.10.1",
		"ms": "^2.1.3",
		"nodemailer": "^7.0.10",
		"otpauth": "^9.4.1",
		"pg": "^8.16.3",
		"react": "19.2.0",
		"react-dom": "19.2.0",
		"uuid": "^13.0.0",
		"winston": "^3.18.3",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.7",
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@commitlint/prompt-cli": "^20.1.0",
		"@react-email/preview-server": "5.0.0",
		"@types/express": "^5.0.4",
		"@types/express-serve-static-core": "^5.1.0",
		"@types/morgan": "^1.9.10",
		"@types/ms": "^2.1.0",
		"@types/node": "^24.9.1",
		"@types/nodemailer": "^7.0.3",
		"@types/pg": "^8.15.6",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@vitest/coverage-v8": "4.0.3",
		"adapter-pg": "link:@types/@prisma/adapter-pg",
		"commitizen": "^4.3.1",
		"husky": "^9.1.7",
		"nodemon": "^3.1.10",
		"prisma": "^7.0.0",
		"react-email": "5.0.0",
		"rimraf": "^6.0.1",
		"supertest": "^7.1.4",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"vite-plugin-static-copy": "^3.1.4",
		"vitest": "^4.0.3"
	},
	"packageManager": "pnpm@10.15.1"
}
